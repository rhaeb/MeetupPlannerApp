{
  "expo": {
    "name": "meet-up-planner",
    "slug": "meet-up-planner",
    "main": "node_modules/expo-router/entry",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "scheme": "meetupplanner",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "48171680-3ad4-4ac1-a2b2-79a8f8756950"
      }
    }
  }
}
