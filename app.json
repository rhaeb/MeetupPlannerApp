{
  "expo": {
    "name": "meet-up-planner",
    "slug": "meet-up-planner",
    "main": "node_modules/expo-router/entry",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.khent1229.meetupplanner"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "scheme": "meetupplanner",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f6f668fd-3548-45a5-8c3e-2fbc2bb4fbc8"
      }
    }
  }
}
